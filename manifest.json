{
  "manifest_version": 3,
  "name": "skip youtube",
  "description": "skip youtube videos",
  "version": "2.0.1",
  "permissions": ["activeTab", "storage", "proxy", "tabs", "background"],
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
